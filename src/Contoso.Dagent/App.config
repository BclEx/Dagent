<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="hosting" type="Contoso.Configuration.HostingSection, Contoso.Dagent"/>
    <section name="appSection" type="Contoso.Configuration.AppSection, Contoso.Dagent"/>
    <section name="rhino.esb" type="Rhino.ServiceBus.Config.BusConfigurationSection, Rhino.ServiceBus"/>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <hosting apiKey="[:ApiKey:]">
    <applications>
      <add key="1234" name="APPID" />
    </applications>
  </hosting>
  <appSection id="[:MachineName:]" log4net="false" >
    <notification fromEmail="info@digitalev.com" subject="Dagent [:MachineName:]" />
    <identity impersonate="false" />
  </appSection>
  <rhino.esb>
    <bus threadCount="1" numberOfRetries="0" endpoint="msmq://localhost/Dagent" />
  </rhino.esb>
  <log4net>
    <appender name="File" type="log4net.Appender.RollingFileAppender">
      <file value="..\log\log.txt" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-5p%d %15.15c %6.6M| %m%n" />
      </layout>
    </appender>
    <root>
      <level value="INFO" />
      <appender-ref ref="File" />
    </root>
  </log4net>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="SpecifiedPickupDirectory">
        <specifiedPickupDirectory pickupDirectoryLocation="C:\inetpub\mailroot\Pickup" />
      </smtp>
    </mailSettings>
  </system.net>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Rhino.Queues" publicKeyToken="0b3305902db7183f" culture="neutral" />
        <bindingRedirect oldVersion="1.4.1.0" newVersion="1.4.2.0"/>
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>